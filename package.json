{
  "name": "vaillovehunt",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "server": "tsx src/server/server.ts",
    "server:dev": "tsx --watch src/server/server.ts",
    "state-server": "node src/server/stateServer.js",
    "state-server:dev": "nodemon src/server/stateServer.js",
    "start": "concurrently --prefix-colors \"cyan,magenta\" --prefix \"[UI],[API]\" \"npm run dev\" \"npm run server:dev\"",
    "start:full": "concurrently --prefix-colors \"cyan,magenta,yellow\" --prefix \"[UI],[API],[STATE]\" \"npm run dev\" \"npm run server:dev\" \"npm run state-server:dev\"",
    "start:netlify": "netlify dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "docusaurus start website",
    "docs:build": "docusaurus build website",
    "docs:serve": "docusaurus serve website",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "api:build": "typedoc"
  },
  "dependencies": {
    "@netlify/blobs": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "cloudinary": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "parse-multipart-data": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.3",
    "resend": "^6.0.3",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@mdx-js/react": "^3.0.1",
    "@netlify/functions": "^4.2.5",
    "@storybook/addon-docs": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "react-docgen-typescript": "^2.2.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^8.3.5",
    "terser": "^5.44.0",
    "tsx": "^4.7.1",
    "typedoc": "^0.26.7",
    "typescript": "^5.3.3",
    "vite": "^5.4.0",
    "vitest": "^3.2.4"
  }
}
